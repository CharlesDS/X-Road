{
  "name": "xroad-centralserver-admin-ui",
  "version": "7.1.0",
  "private": true,
  "scripts": {
    "serve": "rm -rf src/openapi-types && npm run generate-types && vue-cli-service serve",
    "build": "rm -rf src/openapi-types && npm run generate-types && vue-cli-service build",
    "generate-types": "npm run prepare-api-definition && openapi --input ./openapi-definition-frontend.yaml --output ./src/openapi-types/cs-types --exportCore true --client fetch && openapi --input ../../common-rest-api/src/main/resources/common-openapi-definition.yaml --output ./src/openapi-types/common-types --exportCore true --client fetch",
    "test:unit": "vue-cli-service test:unit",
    "test:e2e": "vue-cli-service test:e2e",
    "lint": "vue-cli-service lint",
    "check-audit": "check-audit",
    "resolve-audit": "resolve-audit",
    "license-check": "license-check-and-add check -f license-check-and-add-config.json",
    "license-add": "license-check-and-add add -f license-check-and-add-config.json",
    "prepare-api-definition": "rm -f ./openapi-definition-frontend.yaml* && cp ../admin-service/src/main/resources/openapi-definition.yaml ./openapi-definition-frontend.yaml && sed -i.backup 's+../../../../common-rest-api+../../common-rest-api+g' ./openapi-definition-frontend.yaml"
  },
  "dependencies": {
    "@fontsource/open-sans": "~4.4.5",
    "@niis/shared-ui": "file:../../shared-ui/niis-shared-ui-0.1.0.tgz",
    "axios": "^0.21.1",
    "core-js": "~3.14.0",
    "vee-validate": "3.4.9",
    "vue": "2.6.14",
    "vue-i18n": "~8.24.0",
    "vue-router": "~3.5.1",
    "vuetify": "~2.5.1",
    "vuex": "~3.6.2",
    "vuex-persist": "3.1.3",
    "vuex-router-sync": "5.0.0"
  },
  "devDependencies": {
    "@mdi/font": "~5.9.55",
    "@types/jest": "~26.0.8",
    "@typescript-eslint/eslint-plugin": "~4.26.1",
    "@typescript-eslint/parser": "~4.26.1",
    "@vue/cli-plugin-babel": "~4.5.3",
    "@vue/cli-plugin-e2e-nightwatch": "~4.5.3",
    "@vue/cli-plugin-eslint": "~4.5.3",
    "@vue/cli-plugin-typescript": "~4.5.3",
    "@vue/cli-plugin-unit-jest": "~4.5.3",
    "@vue/cli-service": "~4.5.3",
    "@vue/eslint-config-prettier": "~6.0.0",
    "@vue/eslint-config-typescript": "~7.0.0",
    "@vue/test-utils": "~1.2.0",
    "chromedriver": "^91.0.1",
    "eslint": "~6.8.0",
    "eslint-plugin-prettier": "~3.4.0",
    "eslint-plugin-vue": "~7.10.0",
    "license-check-and-add": "~4.0.2",
    "npm-audit-resolver": "2.3.1",
    "openapi-typescript-codegen": "^0.9.3",
    "prettier": "~2.3.0",
    "sass": "~1.32.6",
    "sass-loader": "~10.1.1",
    "typescript": "4.3.2",
    "vue-cli-plugin-vuetify": "~2.4.0",
    "vue-template-compiler": "2.6.14",
    "vuetify-loader": "~1.7.1"
  }
}
